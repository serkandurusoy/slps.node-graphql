{
  "name": "@sloops/library-ui-components",
  "version": "0.0.1",
  "description": "Sloops repository for reusable visual dumb react components",
  "private": true,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/serkandurusoy/slps.node-graphql.git"
  },
  "author": "Serkan Durusoy <serkan.durusoy@dna-tr.com>",
  "files": [
    "*.js",
    "src/",
    "dist/"
  ],
  "main": "dist/index.js",
  "peerDependencies": {
    "@mygooder/react-slick": "^0.15.4-0.0.2",
    "@sloops/library-utils": "^0.0.1",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.3",
    "react-google-maps": "^9.4.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^2.2.3"
  },
  "dependencies": {
    "react-datetime": "^2.11.0",
    "react-modal": "^3.1.4",
    "react-select": "^1.0.1"
  },
  "devDependencies": {
    "@mygooder/react-slick": "^0.15.4-0.0.2",
    "@sloops/library-utils": "^0.0.1",
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-info": "^3.2.16",
    "@storybook/addon-knobs": "^3.2.16",
    "@storybook/addon-options": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "lodash": "^4.17.4",
    "node-sass": "^7.0.0",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.3",
    "react-google-maps": "^9.4.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "sass-loader": "^6.0.6",
    "slick-carousel": "^1.8.1",
    "styled-components": "^2.2.3",
    "url-loader": "^0.6.2"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ],
      "no-nested-ternary": [
        0
      ],
      "import/prefer-default-export": [
        0
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": true
        }
      ],
      "react/no-unused-prop-types": [
        0
      ],
      "no-unused-prop-types": [
        0,
        {
          "skipShapeProps": true
        }
      ],
      "jsx-a11y/click-events-have-key-events": [
        0
      ],
      "jsx-a11y/anchor-is-valid": [
        0
      ],
      "jsx-a11y/no-noninteractive-element-interactions": [
        0
      ],
      "jsx-a11y/no-static-element-interactions": [
        0
      ],
      "jsx-a11y/anchor-has-content": [
        0
      ],
      "jsx-a11y/no-autofocus": [
        0
      ],
      "class-methods-use-this": [
        0
      ],
      "object-shorthand": [
        "error",
        "always",
        {
          "avoidQuotes": false
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 3 versions",
              "safari >= 7"
            ],
            "uglify": true
          },
          "useBuiltIns": true
        }
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-regenerator",
      "transform-decorators-legacy",
      "transform-export-extensions",
      [
        "lodash",
        {
          "id": [
            "lodash",
            "recompose"
          ]
        }
      ]
    ]
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1"
  },
  "scripts": {
    "link-local-packages": "yarn link && yarn link \"@sloops/library-utils\"",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "yarn run lint && rm -fr dist-storybook && build-storybook -c .storybook -o ./dist-storybook",
    "deploy-storybook": "yarn run build-storybook && bash ../../../scripts/deploy/apps/run.sh ${PWD##*/} staging",
    "deploy-staging": "yarn run deploy-storybook",
    "build-npm": "yarn run lint && rm -fr dist && babel --source-maps inline -d dist/ src/ --copy-files",
    "build": "yarn run build-npm",
    "start": "TZ=Etc/UTC yarn run storybook"
  }
}
