{
  "name": "@sloops/library-ui-data-wrappers",
  "version": "0.0.1",
  "description": "Sloops repository for reusable data wrapper higher order components",
  "private": true,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/serkandurusoy/slps.node-graphql.git"
  },
  "author": "Serkan Durusoy <serkan.durusoy@dna-tr.com>",
  "files": [
    "*.js",
    "src/",
    "dist/"
  ],
  "main": "dist/index.js",
  "peerDependencies": {
    "@sloops/library-utils": "^0.0.1",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "cross-storage": "^1.0.0"
  },
  "devDependencies": {
    "@sloops/library-utils": "^0.0.1",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "lodash": "^4.17.19",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.1.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ],
      "no-nested-ternary": [
        0
      ],
      "import/prefer-default-export": [
        0
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": true
        }
      ],
      "react/no-unused-prop-types": [
        0
      ],
      "no-unused-prop-types": [
        0,
        {
          "skipShapeProps": true
        }
      ],
      "jsx-a11y/click-events-have-key-events": [
        0
      ],
      "jsx-a11y/anchor-is-valid": [
        0
      ],
      "jsx-a11y/no-noninteractive-element-interactions": [
        0
      ],
      "jsx-a11y/no-static-element-interactions": [
        0
      ],
      "jsx-a11y/anchor-has-content": [
        0
      ],
      "jsx-a11y/no-autofocus": [
        0
      ],
      "class-methods-use-this": [
        0
      ],
      "object-shorthand": [
        "error",
        "always",
        {
          "avoidQuotes": false
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 3 versions",
              "safari >= 7"
            ],
            "uglify": true
          },
          "useBuiltIns": true
        }
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-regenerator",
      "transform-decorators-legacy",
      "transform-export-extensions",
      [
        "lodash",
        {
          "id": [
            "lodash",
            "recompose"
          ]
        }
      ]
    ]
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1"
  },
  "scripts": {
    "link-local-packages": "yarn link && yarn link \"@sloops/library-utils\"",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "build-npm": "yarn run lint && rm -fr dist && babel --source-maps inline -d dist/ src/ --copy-files",
    "build": "yarn run lint && yarn run build-npm",
    "start": "yarn run build"
  }
}
